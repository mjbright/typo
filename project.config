[project]
	description = Typo is the oldest and most powerful Ruby on Rails blogware, providing custom templates, powerful drag and drop plugins API, advanced SEO capabilities, XMLRPC API and many more.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Mike BRIGHT (mjbright)
[access "refs/for/refs/*"]
	read = group user/Mike BRIGHT (mjbright)
	push = group user/Mike BRIGHT (mjbright)
	label-Code-Review = -2..+2 group user/Mike BRIGHT (mjbright)
	label-Verified = -1..+1 group user/Mike BRIGHT (mjbright)
	rebase = group user/Mike BRIGHT (mjbright)
	removeReviewer = group user/Mike BRIGHT (mjbright)
	submit = group user/Mike BRIGHT (mjbright)
[access "refs/heads/*"]
	read = group user/Mike BRIGHT (mjbright)
	create = group user/Mike BRIGHT (mjbright)
	push = +force group user/Mike BRIGHT (mjbright)
	pushMerge = group user/Mike BRIGHT (mjbright)
[access "refs/tags/*"]
	push = +force group user/Mike BRIGHT (mjbright)
	pushTag = group user/Mike BRIGHT (mjbright)
	pushSignedTag = group user/Mike BRIGHT (mjbright)
